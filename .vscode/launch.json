  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "beebop/app.py",
        "FLASK_DEBUG": "1",
        "STORAGE_LOCATION": "./storage",
        "DBS_LOCATION": "./storage/dbs"
      },
      "args": ["run", "--no-debugger", "--no-reload"],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "rq worker",
      "request": "launch",
      "type": "debugpy",
      "program": "/home/athapar/miniconda3/envs/beebop_py/bin/rq",
      "args": ["worker"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false
    },
    {
      "name": "poppunk_assign sub lineage test",
      "type": "debugpy",
      "request": "launch",
      "program": "/home/athapar/miniconda3/envs/beebop_py/bin/poppunk_assign",
      "args": [
        "--model-dir",
        "${workspaceFolder}/storage/dbs/GPS_v9_lineages/GPS_v9_lineages_10_lineage_db",
        "--db",
        "${workspaceFolder}/storage/dbs/GPS_v9",
        "--query",
        "${workspaceFolder}/qfile.txt",
        "--output",
        "${workspaceFolder}/storage/poppunk_output/test1/gpsc8"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false
    }
  ]
}
