{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "hash": {
      "type": "string"
    },
    "samples": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z0-9]*$": {
          "type": "object",
          "properties": {
            "hash": {
              "type": "string"
            },
            "cluster": {
              "type": ["integer", "string"]
            },
            "sketch": { "$ref": "sketch.schema.json" },
            "failedReasons": {
              "type": "string"
            }
          },
          "required": ["hash"],
          "additionalProperties": false
        }
      }
    },
    "status": { "$ref": "status.schema.json" }
  },
  "required": ["hash", "samples", "status"],
  "additionalProperties": false
}