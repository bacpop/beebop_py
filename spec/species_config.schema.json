{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "properties": {
        "hasSublineages": {
          "type": "boolean"
        },
        "kmerInfo": {
          "type": "object",
          "properties": {
            "kmerMax": {
              "type": "integer",
              "minimum": 1
            },
            "kmerMin": {
              "type": "integer",
              "minimum": 1
            },
            "kmerStep": {
              "type": "integer",
              "minimum": 1
            }
          },
          "required": ["kmerMax", "kmerMin", "kmerStep"],
          "additionalProperties": false
        },
        "locationMetadata": {
          "type": ["array", "null"],
          "items": {
            "type": "object",
            "properties": {
              "SampleCounts": { "type": "integer" },
              "Latitude": { "type": "number" },
              "Longitude": { "type": "number" }
            }
          }
        }
      },
      "required": ["hasSublineages", "kmerInfo"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
